class Solution {
public:
    vector<vector<int>> missingRanges(vector<int> &arr, int lower, int upper) {
        vector<vector<int>> result;
        
        for (int i = 0; i < arr.size(); i++) {
       
            if (arr[i] > lower) {
                result.push_back({lower, arr[i] - 1});
            }
          
            lower = arr[i] + 1;
        }

        if (lower <= upper) {
            result.push_back({lower, upper});
        }

        return result;
    }
};
