// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
int arr[5]={1,4,5,9,8};
int maxvalue=-1;
for(int i=0;i<5;i++){
    if(arr[i]>maxvalue)
    maxvalue=arr[i];
}

int second=-1;
for(int i=0;i<5;i++){
    if(arr[i]!=maxvalue)
    second=max(second,arr[i]);
}


int third=-1;
for(int i=0;i<5;i++){
    if(arr[i]!=maxvalue && arr[i]!=second)
    third=max(third,arr[i]);
    }
    
    cout<<third;
    return 0;
    }
